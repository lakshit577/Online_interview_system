<%= form_with(model: @student, url: students_path, local: true) do |form| %>
 
  <%= form.label :fullname %>
  <%= form.text_field :fullname %>

  <%= form.label :email %>
  <%= form.email_field :email %>


  <%= form.label :contact_number %>
  <%= form.text_field :contact_number %>

  <%= form.submit "start now" %>
<% end %>






